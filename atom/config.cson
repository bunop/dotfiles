"*":
  "atom-ide-ui":
    "atom-ide-terminal":
      rendererType: "dom"
  "atom-typescript":
    autocompleteParens: true
    checkAllFilesOnSave: true
  core:
    customFileTypes:
      "source.ts": [
        "ts"
      ]
    disabledPackages: [
      "linter"
    ]
    telemetryConsent: "no"
    useTreeSitterParsers: false
  editor:
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "c7cdac1d-ab49-4d3e-911b-49de51370399"
  "file-types":
    "*.component.html": "text.html.ng"
  kite:
    showWelcomeNotificationOnStartup: false
  "linter-ui-default": {}
  welcome:
    showOnStartup: false
